var a = "amSTerDaM"
undefined
a.charAt(0)
'a'
a.charAt(0).toUpperCase()
'A'
a.slice(1)
'mSTerDaM'
a.slice(1).toLowerCase()
'msterdam'
a.charAt(0).toUpperCase()+a.slice(1).toLowerCase()
'Amsterdam'
var a = "pARis"
undefined
a.charAt(0).toUpperCase()+a.slice(1).toLowerCase()
'Paris'


